{
  "manifest_version": 3,
  "name": "Page Mover",
  "description": "This extension can move pages by hotkeys, such as pressing 'w' or 'k' to move pages up and 's' or 'j' to  move pages down.",
  "version": "0.9.8",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "action": {
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "event.js"
  },
  "permissions": [
    "tabs", "contextMenus"
  ],
  "icons":{
    "16": "icon.png"
  },
  "commands": {
    "activePageMoverCommand": {
      "suggested_key": {
        "default": "Alt+Q"
      },
      "description": "Toggle this extension"
    },
    "newTabCommand": {
      "suggested_key": {
        "default": "Alt+N"
      },
      "description": "Create new tab"
    },
    "switch_icon": {
      "suggested_key": {
        "default": "Alt+T"
      },
      "description": "switch this action icon"
    }
  }
}